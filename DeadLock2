  static void printP(int sr,int sc,int er,int ec,String s,int[][]maze) {
        if(sr>er||sc>ec) return;
        if(sr<0||sc<0) return;
        if(sr==er&&sc==ec){
            System.out.println(s);
            return;
        }
        if(maze[sr][sc]==0)return;

        printP(sr,sc+1,er,ec,s+"R",maze);
        printP(sr+1,sc,er,ec,s+"D",maze);
    }
